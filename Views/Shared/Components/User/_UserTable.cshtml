@model List<panasonic.Models.User>;


<table class="table table-hover">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Full Name</th>
            <th>Email</th>
            <th>Role</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in @Model)
        {
            <tr>
                <td>@user.EmployeeID</td>
                <td>@user.Fullname</td>
                <td>@user.Email</td>
                <td>@user.Role.DisplayName</td>
                <td>
                    <a class="btn btn-primary" asp-controller="User" asp-action="Detail" asp-route-id="@user.Id">Details</a>
                </td>
            </tr>


        }

    </tbody>
</table>
